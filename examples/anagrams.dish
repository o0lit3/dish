// https://rosettacode.org/wiki/Anagrams
// curl -s "http://wiki.puzzlers.org/pub/wordlists/unixdict.txt" | dish examples/anagrams.dish

stdin.map:word(
  sorted: word.sort
  anagram.sorted << word
  (#anagram.sorted > max).then(max = #anagram.sorted)
)

anagram.values.filter:set(
  #set == max
).map:set(
  set.join(', ')
).sort.join("\n")
