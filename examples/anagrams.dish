// curl -s "http://wiki.puzzlers.org/pub/wordlists/unixdict.txt" | dish examples/anagrams.dish

stdin.map:word(
  sorted = word.sort
  anagram.sorted << word
  (#(anagram.sorted) > max).and(max = #(anagram.sorted))
)

anagram.values.grep:set(
  #set == max
).map:set(
  set.join(", ")
).sort.join("\n")
