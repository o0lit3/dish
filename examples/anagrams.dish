## curl -s "http://wiki.puzzlers.org/pub/wordlists/unixdict.txt" | dish examples/anagrams.dish

stdin.map:word(
  anagram.(word.sort) << [word]
)

max = anagram.map:set(#set).max

anagram.values.filter:set(
  #set == max
).map:set(
  set.join(', ')
).sort.join
