// https://rosettacode.org/wiki/CRC-32

[0, 1, 2, 4, 5, 7, 8, 10, 11, 12, 16, 22, 23, 26, 32].each:i(
  divisor += (1 << (32 - i))
)

table = (0..255).map:rem(
  (0..7).each:i(
    rem.i.then(
      rem ^= (divisor << i)
    )
  )

  rem >> 8
)

crc = :s(
  out: 0

  s.each:c(
    out = (out >> 8) ^ (table.(out & 255) ^ c)
  )

  out
)

"The quick brown fox jumps over the lazy dog".crc
