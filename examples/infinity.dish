// https://rosettacode.org/wiki/Infinity

[
  ["i", i = inf]
  ["i == inf", i == inf]
  ["inf > 1", inf > 1]
  ["-inf > 1", -inf > 1]
  ["inf + inf", inf + inf]
  ["inf - inf", inf - inf]
  ["inf * inf", inf * inf]
  ["inf / inf", inf / inf]
  ["inf + 2", inf + 2]
  ["inf / 2", inf / 2]
  ["2 / inf", 2 / inf]
  ["1 / 0", 1 / 0]
].map:i(
  (i.0 := i.0.fmt('%10s')).join(" \t// ") 
).join("\n")
