// https://rosettacode.org/wiki/Least_common_multiple

gcd = :n:m(
  << (m.redo([n, m] = [m, n % m]))
)

lcm = :n:m(
  (n && m).then((n / n.gcd(m) * m).abs).else(0)
)


[
  {a: 0, b: 35}
  {a: 12, b: 18}
  {a: 14, b: -6}
  {a: -5, b: 12}
  {a: 1001, b: 221}
  {a: 1764, b: 3920} 
].map:obj(
  "$(obj.a).lcm($(obj.b))" ~ " \t// " ~ obj.a.lcm(obj.b)
).join("\n")
