// https://rosettacode.org/wiki/Least_common_multiple

gcd = :n:m(
  << (m.redo([n, m] = [m, n % m]))
)

lcm = :n:m(
  (n && m).then((n / n.gcd(m) * m).abs).else(0)
)


[
  [0, 35]
  [12, 18]
  [14, -6]
  [-5, 12]
  [1001, 221]
  [1764, 3920]
].map:i(
  "$(i.0).lcm($(i.1)) = $(i.0.lcm(i.1))"
).join("\n")
