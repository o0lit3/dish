## https://rosettacode.org/wiki/Least_common_multiple

gcd = :n:m(
  << (m.redo:([n, m] = [m, n % m]))
)

lcm = :n:m(
  (n && m).switch[(n / n.gcd(m) * m).abs, 0]
)

[
  '0.lcm(35)'
  '12.lcm(18)'
  '14.lcm(-6)'
  '-5.lcm(12)'
  '1001.lcm(221)'
  '1764.lcm(3920)'
].map:i(
  "$i \t## $(i.eval)"
).join("\n")
