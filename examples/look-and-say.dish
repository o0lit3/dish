// https://rosettacode.org/wiki/Look-and-say_sequence

look$say = :s(
  [out, digit, count]: ["", s.0, 1]

  (<< s).redo(
    (s.0 == digit).then(++count).else(out ~= "$(count)$(digit)", digit = s.0, count = 1)
  )
  
  out
)


(
  [s = 1] + (
    (0..13).map:(
      s = s.look$say 
    )
  )
).join("\n")
