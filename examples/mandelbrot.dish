// https://rosettacode.org/wiki/Mandelbrot_set

x = y = -16
out = ['']

(++y < 16).redo(
  x = 0
  (++x < 85).redo(
    i = k = r = 0

    (
      j = (r * r - i * i - 2 + x / 25).round(10)
      i = (2 * r * i + y / 10).round(10)
      j * j + i * i < 11 && ++k < 112
    ).redo(r = j)

    out.-1 ~= " .:-;!/>)|&IH%*#".(k & 15)
  )

  out << ''
)

out.join("\n")
