## https://rosettacode.org/wiki/SEDOLs

chars = (0..9) + ('A'..'Z').grep:c(c.consonant)
weights = [1, 3, 1, 7, 3, 9]

checksum = :s:sum(
  s.each:c:i(
    sum += (c#36) * weights.i
  )

  (10 - (sum % 10)) % 10
)

[
  '710889'
  'B0YBKJ'
  '406566'
  'B0YBLH'
  '228276'
  'B0YBKL'
  '557910'
  'B0YBKR'
  '585284'
  'B0YBKT'
  'B00030'
].map:s(
  "$s \t $(s + *s.checksum)"
).join("\n")
