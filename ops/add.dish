[
    [1, 2, 3] + [2, 4]          # [1, 2, 3, 2, 4]
    [1, 2, 3] + "foo"           # [1, 2, 3, "foo"]
    "bing" + {foo: 1, bar: 2}   # 2
    "2" + "3.14"                # 5.14
    3 + 4                       # 7
    4 + true                    # 5
    true + false                # true
    false + false               # false
]
