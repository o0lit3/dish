[
    [1, 2] / "foo"      // null
    [1, 2, 3] / [1, 2]  // [[1, 2], [3]]
    [1, 2, 3] / true    // [[1, 2, 3]]
    "bing" / [3.14, 1]  // ["bi", "ng"]
    "bing" / 1.5        // ["bing"]
    "bing" / false      // null
    "foobar" / 4        // ["fo", "ob", "a", "r"]
    1..3.split(3)       // [[1], [2], [3]]
    2 / 3.14            // 0.636942675159
    4 / 2               // 2
    2 / 2 / 2           // 0.5
    3 + 2 / 2           // 4
    true / [4, 5]       // 0.5
]
