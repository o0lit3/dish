[
    [1, 2, 3]~"!"                   # "1!2!3"
    [4, 5, 6]~true                  # "4true5true6"
    ["a", "b", 4, "c"].join("!")    # "a!b!4!c"
    "3.14"~"2"                      # "3.142"
    2~2                             # "22"
    "foo"~"bar"                     # "foobar"
    "foo"~[1, 2, 3]                 # "foo[1, 2, 3]"
    "bi"~["n", "g"]                 # "bi[\"n\", \"g\"]"
    true~false                      # "truefalse"
]
