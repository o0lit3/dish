[
    [1, 2] * "foo"      # []
    [1, 2, 3] * "1.5"   # [1, 2, 3, 1, 2]
    [1, 2, 3] * true    # [1, 2, 3]
    "bing" * [3.14, 1]  # "bingbing"
    "bing" * 1.5        # "bingbi"
    "bing" * false      # ""
    3 * { ++a }         # [1, 2, 3]
    2 * 3.14            # 6.28
    2 * 2 * 2           # 8
    3 + 2 * 2           # 7
    true * [4, 5]       # [4, 5]
    [3, 4] * { _ * @ }  # [0, 4]
]
