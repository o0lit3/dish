[
    [1, 2] * "foo"      // []
    [4, 4] * [1, 2, 3]  // [[1, 2, 3], [1, 2, 3]]
    [1, 2, 3] * "1.5"   // [1, 2, 3, 1, 2]
    [1, 2, 3] * true    // [1, 2, 3]
    "bing" * ($1)       // ["b", "i", "n", "g"]
    "bing" * 1.5        // "bingbi"
    "bing" * false      // ""
    1..3.map(++a)       // [1, 2, 3]
    2 * 3.14            // 6.28
    2 * 2 * 2           // 8
    3 + 2 * 2           // 7
    true * [4, 5]       // [4, 5]
    [3, 4] * ($1 * $2)  // [0, 4]
]
