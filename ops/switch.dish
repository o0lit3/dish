[
    [1, 2, 3].switch[1, 2, 3]       // 1
    [0, 0, 0].switch[1, 2, 3]       // 3
    [0, 1, 0].switch[1, 2, 3]       // 2
    [0, ++a, ++b]?[0, [a, b]]       // [1, null]
    ([++d, ++c]?[++c, ++d], [c, d]) // [1, 1]
    ([e, ++f]?[++e, ++f], [e, f])   // [null, 2]
    true.switch[4, 5]               // 4
    false.switch[4, 5]              // 5
    1.switch[4, 5]                  // 4
    0.switch[4, 5]                  // 5
]
