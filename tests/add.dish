[
    {} + {foo: 1}               // {"foo": 1}
    [1, 2, 3] + [2, 4]          // [1, 2, 3, 2, 4]
    [1, 2, 3] + "foo"           // [1, 2, 3, "foo"]
    "foo" + [1, 2, 3]           // 3
    2 + [1, 2, 3]               // 5
    [1, 2, 3] + 2               // [1, 2, 3, 2]
    "2" + "3.14"                // 5.14
    3 + 4                       // 7
    4 + true                    // 5
    3.14 + 2                    // 5.14
    true + false                // 1
    false + false               // 0
    [[1, 2]] + [1, 2, 3]        // [[1, 2], 1, 2, 3]
]
