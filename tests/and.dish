[
    {} && true              // false
    {foo: 0} && true        // true
    [] && true              // []
    true && [0, 0]          // [0, 0]
    "0" && true             // true
    true && "foo"           // "foo"
    "" && true              // ""
    true && 3.14            // 3.14
    0 && true               // 0
    true && true            // true
    [false && ++a, a]       // [false, null]
    [true && ++b, b]        // [1, 1]
    [true && ++c && ++c, c] // [2, 2]
    [1 && 0 || ++d, d]      // [1, 1]
    [1 && 1 || ++e, e]      // [1, null]
]
