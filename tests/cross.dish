[
    {} + {foo: 1}           ## {"foo": 1}
    [1, 2, 3] + [2, 4]      ## [1, 2, 3, 2, 4]
    2 + [1, 2, 3]           ## [null, null, 1, 2, 3]
    [1, 2, 3] + 2           ## [1, 2, 3, null, null]
    [1, 2, 3] + :x:y(x + y) ## [1, 3, 6]
    '2' + '3.14'            ## "23.14"
    "foobar" + 2            ## "foobat"
    3 + 4                   ## 7
    4 + true                ## 5
    3.14 + 2                ## 5.14
    true + false            ## 1
    false + false           ## 0
    [[1, 2]] + [1, 2, 3]    ## [[1, 2], 1, 2, 3]
    (a = [1, 2, 3], ++a)    ## [1, 2, 3, null]
    (a = "foobar", ++a)     ## "foobas"
    (a = 3.14, ++a)         ## 4.14
    (a = 2, ++a)            ## 3
    (a = false, ++a)        ## 1
    ++var                   ## 1
    ++var                   ## 2
    {foo: 2, bar: 3}.sum    ## 5
    [1, 2, 3].sum           ## 6
    +[1, 2, 3]              ## 6
    +"foobar"               ## 0
    +3.14                   ## 3.14
    +5                      ## 5
    "2.50".num              ## 2.5
    +true                   ## 1
    false.num               ## 0
    +null                   ## 0
    +[1, null, 3]           ## 4
]
