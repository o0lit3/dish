[
    [1, 2] / "foo"          // null
    [1, 2, 3] / "2"         // [[1, 2], [3]]
    [1, 0, 1, 2].split(!$_) // [[1], [1, 2]]
    [1, 2, 3] / true        // [[1, 2, 3]]
    "bing" / 2              // ["bi", "ng"]
    "bing" / ($_ == "i")    // ["b", "ng"]
    "bing" / 1.5            // ["bing"]
    "bing" / false          // null
    "foobar" / 4            // ["fo", "ob", "a", "r"]
    "foobar" / "o"          // ["f", "", "bar"]
    "foobar" / "oo"         // ["f", "bar"]
    "foobar" / ""           // ["f", "o", "o", "b", "a", "r"]
    (1..3).split(2)         // [[1], [3]]
    (2 / 3.14) @ 12         // 0.636942675159
    4 / 2                   // 2
    2 / 2 / 2               // 0.5
    3 + 2 / 2               // 4
    true / [4, 5]           // 0.5
]
