[
    {foo: 1, bar: 2} @ 2            // ["bar"]
    {foo: 1, bar: 2} @ 3            // []
    [4, 5, 6].find(6)               // [2]
    [4, 5, 6].find:i(i % 3 == 0)    // [2]
    [4, 5, 6] @ 5                   // [1]
    [4, 5, 6] @ 7                   // []
    [1, 0, 1] @ 1                   // [0, 2]
    "foobar" @ "a"                  // [4]
    "foobar" @ "o"                  // [1, 2]
    "foobar" @ "bar"                // [3]
    ?("foobar" @ "bar")?[1, 0]      // 1
    ?("foobar" @ "baz")?[1, 0]      // 0
    "100110" @ 1                    // [0, 3, 4]
    "100110" @ :(!$_)               // [1, 2, 5]
    3.1415926535 @ 2                // 3.14
    true @ false                    // 1
]
