[
    {foo: 1, bar: 0}.grep:($1)      ## {"foo": 1}
    {foo: 1, bar: 2} % :(!($1 % 2)) ## {"bar": 2}
    [1, 2, 3, 4, 5] % 2             ## [1, 3, 5]
    [0, 1, 6, 0, 9].grep:($1)       ## [1, 6, 9]
    [1, 2, 3] % false               ## []
    [-1, 0, 4, -4, 1].grep:n(n < 0) ## [-1, -4]
    "foobar" % :($1 == 'o')         ## "oo"
    "foobar" % 2                    ## "foa"
    15 % 3                          ## 0
    13 % 3                          ## 1
    -13 % 3                         ## 2
    13 % -3                         ## 1
    -13 % -3                        ## -1
    15.5 % 3.14                     ## 2.94
    15.5 % 4.5                      ## 2
    true % 4                        ## 1
    false % 4                       ## 0
    {foo: 1, bar: 0}.without:($1)   ## {"bar": 0}
    {foo: 1, bar: 2} %% :($1 % 2)   ## {"bar": 2}
    [1, 2, 3, 4, 5] %% 2            ## [2, 4]
    [0, 1, 6, 0, 9] %% :($1)        ## [0, 0]
    [1, 2, 3] %% false              ## [1, 2, 3]
    [-1, 0, 4, -4, 1] %% :n(n < 0)  ## [0, 4, 1]
    "foobar" %% :($1 == 'o')        ## "fbar"
    "foobar" %% 2                   ## "obr"
    15 %% 3                         ## 0
    13 %% 3                         ## 1
    -13 %% 3                        ## 2
    -13 %% -3                       ## -1
    15.5 %% 3.14                    ## 2
    15.5 %% 4.5                     ## 2
    true %% 4                       ## 1
    false %% 4                      ## 0
    %{x: 1, y: 2}                   ## {"x": 1, "y": 2}
    %[1, 2]                         ## {"1": true, "2": true}
    %"foobar"                       ## {"foobar": true}
    %3.14                           ## {"denom": 50, "num": 157}
    %3                              ## {"denom": 1, "num": 3}
    %true                           ## {"denom": 1, "num": 1}
    %null                           ## {"denom": 1, "num": 0}
]
