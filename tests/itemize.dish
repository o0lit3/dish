[
    /{foo: 1}   // [1]
    /[1, 2, 3]  // [1, 2, 3]
    /"foobar"   // ["f", "o", "o", "b", "a", "r"]
    3.14.ratio  // [157, 50]
    /3.14       // [157, 50]
    /~3.14      // ["3", ".", "1", "4"]
    /5          // [5, 1]
    /false      // [0, 1]
    /-4         // [-4, 1]
    /~-4        // ["-", "4"]
]
