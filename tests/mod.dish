[
    {foo: 1, bar: 0}.grep:($_)      // {"foo": 1}
    {foo: 1, bar: 2} % :(!($_ % 2)) // {"bar": 2}
    [1, 2, 3, 4, 5] % 2             // [1, 3, 5]
    [1, 2, 3, 4, 5] % "3"           // [1, 4]
    [0, 1, 6, 0, 9].grep:($_)       // [1, 6, 9]
    [1, 2, 3] % false               // null
    [-1, 0, 4, -4, 1].grep:n(n < 0) // [-1, -4]
    "foobar" % "bar"                // null
    "foobar" % :($_ == 'o')         // "oo"
    "foobar" % 2                    // "foa"
    "foobar" % "3"                  // "fb"
    15 % 3                          // 0
    15 % 4                          // 3
    15.5 % 3.14                     // 0
    15.5 % 4.5                      // 3
    15 % false                      // null
    true % 4                        // 1
    false % 4                       // 0
]
