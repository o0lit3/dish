[
    (a = {foo: 1}, [>> a, a])       // [1, {}]
    (a = [1, 2, 3], [>> a, a])      // [3, [1, 2]]
    (a = "foobar", [>> a, a])       // ["r", "fooba"]
    (a = 3.14, [>> a, a])           // [3.14, 3.14]
    (a = ["f", 1], [>> a, a])       // [1, ["f"]]
    (a = [], [>> a, a])             // [null, []]
    (a = [1, 2, 3], [a.pop, a])     // [3, [1, 2]]
    (a = [1, 2, 3], [a.last, a])    // [3, [1, 2, 3]]
]
