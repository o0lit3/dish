[
    [1, 2, 3] ^ 2                           ## [2, 3, 1]
    [1, 2, 3] ^ -2                          ## [3, 1, 2]
    ["10", "1", "2", "4"].sort:($1 < $2)    ## ["1", "10", "2", "4"]
    ["10", "1", "2", "4"] ^ :(+$1 < +$2)    ## ["1", "2", "4", "10"]
    [10, 1, 2, 4].sort:a:b(*a < *b)         ## [1, 10, 2, 4]
    [10, 1, 2, 4] ^ :a:b(a < b)             ## [1, 2, 4, 10]
    [1, 2, 3] ^ [4, 5]                      ## [[1, 4], [2, 5], [3]]
    "ds" ^ "ih"                             ## "dish"
    "foobar" ^ :($1 < $2)                   ## "abfoor"
    "foobar" ^ 2                            ## "arfoob"
    "foobar" ^ -2                           ## "obarfo"
    3.14 ^ 2                                ## 9.8596
    5 ^ 2                                   ## 25
    25 ^ 0.5                                ## 5
    true ^ 2                                ## 1
    false ^ 2                               ## 0
    2 ^ 3                                   ## 8
    ["10", "1", "2", "4"].sort              ## ["1", "10", "2", "4"]
    ^["10", "1", "2", "4"]                  ## ["1", "10", "2", "4"]
    [10, 1, 2, 4].sort                      ## [1, 2, 4, 10]
    ^[10, 1, 2, 4]                          ## [1, 2, 4, 10]
    "foobar".sort                           ## "abfoor"
    ^"foobar"                               ## "abfoor"
    ^8                                      ## 64
    ^10.5                                   ## 110.25
    ^true                                   ## 1
]
