[
    [1, 2, 3] ** 2                      // [2, 3, 1]
    [1, 2, 3] ** -2                     // [3, 1, 2]
    ["10", "1", "2", "4"].sort($1 < $2) // ["1", "10", "2", "4"]
    ["10", "1", "2", "4"]**(+$1 < +$2)  // ["1", "2", "4", "10"]
    [10, 1, 2, 4].sort:a:b(~a < ~b)     // [1, 10, 2, 4]
    [10, 1, 2, 4]**:a:b(a < b)          // [1, 2, 4, 10]
    "foobar"**($1 < $2)                 // "abfoor"
    "foobar" ** 2                       // 0
    ~(/"foobar" ** 2)                   // "arfoob"
    ~(/"foobar" ** -2)                  // "obarfo"
    "2" ** 2                            // 4
    2 ** "2"                            // 4
    3.14 ** 2                           // 9.8596
    5 ** 2                              // 25
    25 ** 0.5                           // 5
    true ** 2                           // 1
    false ** 2                          // 0
    2 ** 3                              // 8
    2 ** "foobar"                       // 1
]
