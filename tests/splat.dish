[
    {foo: 1}.map:(++$1)             ## {"foo": 2}
    {foo: 1} * :[1, 2]              ## {"foo": [1, 2]}
    {foo: 1} * :(3)                 ## {"foo": 3}
    [0] * 4                         ## [0, 0, 0, 0]
    [[0] * 2] * 2                   ## [[0, 0], [0, 0]]
    [1, 2] * " "                    ## "1 2"
    [4, 4] * [1, 2]                 ## [4, 8]
    [1, 2, 3] * 1.5                 ## [1, 2, 3, 1]
    [1, 2, 3] * :($1 * 1.5)         ## [1.5, 3, 4.5]
    1.5 * [1, 2, 3]                 ## [1, 2, 3, 1]
    [1, 2, 3] * true                ## [1, 2, 3]
    "bing" * 1.5                    ## "bingbi"
    "bing" * false                  ## ""
    "bottle" + ("s" * true)         ## "bottles"
    (1..3).map:(++a)                ## [1, 2, 3]
    2 * 3.14                        ## 6.28
    2 * 2 * 2                       ## 8
    3 + 2 * 2                       ## 7
    true * [4, 5]                   ## [4, 5]
    [3, 4] * :val:idx(val * idx)    ## [0, 4]
    [[3 1], [8 2]] * [[6 1], [7 9]] ## [[25, 12], [62, 26]]
    [2 1] * [3, -2]                 ## [6, -2]
    +([2 1] * [3, -2])              ## 4
    [1, 2, 3] ** 2                  ## [[1, 2], [1, 3], [2, 3]]
    'bin' ** 2                      ## ["bi", "bn", "in"]
    10.5 ** 2.5                     ## 45
    10 ** 2                         ## 45
    *{foo: 1, bar: 2}               ## 2
    *[1, 2, 3]                      ## 6
    *['foo', 'bar']                 ## "foobar"
    *'foobar'                       ## "foobar"
    *7                              ## "7"
    *3.14                           ## "3.14"
    *true                           ## "true"
    false.str                       ## "false"
    (-4).str                        ## "-4"
    *-4                             ## "-4"
    *null                           ## "null"
    **[1, 2]                        ## [[1, 2], [2, 1]]
    **'bin'                         ## ["bin", "ibn", "nib", "inb", "nbi", "bni"]
    **12                            ## [1, 2, 3, 4, 6]
]

