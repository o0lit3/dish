[
    ~{foo: 1}               // "1"
    ~[1, 2, 3]              // "123"
    ~["b", "i", "n", "g"]   // "bing"
    ~"foobar"               // "foobar"
    ~3.14                   // "3.14"
    ~5                      // "5"
    ~true                   // "true"
    false.str               // "false"
    (-4).str                // "-4"
    ~-4                     // "-4"
]
