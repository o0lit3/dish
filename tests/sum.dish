[
    {foo: 2, bar: 3}.sum    // 5
    [1, 2, 3].sum           // 6
    +[1, 2, 3]              // 6
    +"foobar"               // 0
    +3.14                   // 3.14
    +5                      // 5
    "2.50".num              // 2.5
    +true                   // 1
    false.num               // 0
    +null                   // 0
    +[1, "foo", 3.14]       // 4.14
]
